<table class="table table-striped">
  <thead>
    <tr>
      <th> <%= 'Descrição do produto' %></th>
      <th> <%= 'Estoque' %></th>
      <th> <%= '10T R$' %></th>
      <th> <%= '11/20T R$' %></th>
      <th> <%= '21/30T R$' %></th>
      <th> <%= '31/40T R$' %></th>
      <th> <%= '+ 40T R$' %></th>
     
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
      
    </tr>
  </thead>
  <tbody>
 
 	<% @client.prod_clis.each do |p| %>
        <tr>
        <td><%= p.product.name %></td>
        <td><%= p.qnt %></td>
        <td><%= p.val1 %></td>
        <td><%= p.val2 %></td>
        <td><%= p.val3 %></td>
        <td><%= p.val4 %></td>
        <td><%= p.val5 %></td>
        <td><%= link_to "Editar produto", edit_prod_cli_path(:id_prod_cli => p, :product => p.product.name), :class => 'btn btn-default btn-xs' %></td>
        </tr>
      <% end %>
      </tbody>
   </table>
