<h5>Adicionar item</h5>
<%= form_for ([@outtowel, @outtowel.itemouts.build]) do |f| %>

<% if @outtowel.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@outtowel.errors.count, "error") %> prohibited this heritage from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @outtowel.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>
 
  <div class="col-lg-1">
	<%= f.number_field :qnt, class: "form-control", placeholder: "qnt", id: 'quant' -%>
	</div>
	
	<div class="col-lg-3">
    <%= f.collection_select(:product_id, @products, :id, :name, {include_blank: 'Selecione o item'},:class => 'form-control', id: 'selectprod') %>
    </div>
    	
    <%= f.submit 'Adicionar', class: 'btn btn-primary' %>
       
<% end %>



